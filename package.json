{
  "name": "awesome-ui", "version": "0.1.1",
  "private": true, "license": "GPL",

  "scripts": {
    "install": "bower install",
    "postinstall": "npm run-script build",
    "build": "grunt",
    "start": "grunt dev"
  },

  "build": {
    "font": {
      "source"    : "bower_components/font-awesome/font",
      "target"    : "font"
    },
    "angular": {
      "source"    : "bower_components/angular",
      "target"    : "js/libs/angular"
    },
    "angularAnimate": {
      "source"    : "bower_components/angular-animate",
      "target"    : "js/libs/angular"
    },
    "angularResource": {
      "source"    : "bower_components/angular-resource",
      "target"    : "js/libs/angular"
    },
    "angularRoute": {
      "source"    : "bower_components/angular-route",
      "target"    : "js/libs/angular"
    },
    "angularLocale": {
      "locale"    : "angular-locale_en-us.js",
      "source"    : "bower_components/angular-i18n",
      "target"    : "js/libs/angular"
    },
    "awesome": {
      "css": {
        "source"    : "components/awesome/css",
        "target"    : "css"
      },
      "js": {
        "source"    : "components/awesome/js",
        "target"    : "js"
      },
      "templates": {
        "source"    : "components/awesome",
        "target"    : "./"
      }
    },
    "bootstrap": {
      "source"    : "bower_components/bootstrap",
      "target"    : "js/libs/bootstrap"
    },
    "jquery": {
      "source"    : "bower_components/jquery",
      "target"    : "js/libs/jquery"
    }
  },

  "devDependencies": {
    "coffee-script": "latest",
    "bower": "latest",
    "grunt": "latest",
    "grunt-contrib-clean": "latest",
    "grunt-contrib-watch": "latest",
    "grunt-contrib-coffee": "latest",
    "grunt-contrib-copy": "latest",
    "grunt-contrib-jade": "latest",
    "grunt-contrib-less": "latest"
  }
}
